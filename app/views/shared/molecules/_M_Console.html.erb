<div class="M_Console" id="js-console" data-js-console aria-hidden="true" style="display: none;">
  <div class="M_Console-Header" data-js-console-drag>
    <%= render 'shared/quarks/Q_Icon', name: 'Q_ConsoleIcon', width: 18, height: 18 %>
    <div class="M_Console-Header-Controls">
      <button type="button" class="M_Console-Header-Button M_Console-Header-Button--maximize" data-js-console-maximize aria-label="На весь экран">
        <span class="M_Console-Header-Button-Icon M_Console-Header-Button-Icon--maximize"></span>
      </button>
      <button type="button" class="M_Console-Header-Button M_Console-Header-Button--close" data-js-console-close aria-label="Закрыть">
        <span class="M_Console-Header-Button-Icon M_Console-Header-Button-Icon--close">×</span>
      </button>
    </div>
  </div>
  <div class="M_Console-Body">
    <div class="M_Console-Lines" data-js-console-line-numbers aria-hidden="true">
      <span class="M_Console-LineNum text-code" data-js-console-ln>01</span>
    </div>
    <div class="M_Console-Content" data-js-console-output>
      <div class="M_Console-Output" data-js-console-lines></div>
      <div class="M_Console-InputLine text-code" contenteditable="true" data-js-console-input spellcheck="false" data-placeholder=" " aria-label="Команда" role="textbox">>_ </div>
    </div>
    <div class="M_Console-Resize M_Console-Resize--right" data-js-console-resize-right aria-label="Изменить ширину"></div>
    <div class="M_Console-Resize M_Console-Resize--bottom" data-js-console-resize-bottom aria-label="Изменить высоту"></div>
    <div class="M_Console-Resize M_Console-Resize--corner" data-js-console-resize-corner aria-label="Изменить размер"></div>
  </div>
</div>
