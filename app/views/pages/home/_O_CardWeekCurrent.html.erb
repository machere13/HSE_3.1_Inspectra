<%= link_to week_path(week), class: "O_CardWeekCurrent" do %>
    <div class="O_CardWeekCurrent-Header">
        <%= render 'shared/quarks/Q_Icon', name: 'Q_CurrentWeekIcon', width: 6, height: 9 %>
        <span class="O_CardWeekCurrent-Label text-labels-1"><%= "WEEK #{week.number}" %></span>
    </div>
    <div class="O_CardWeekCurrent-Body" data-week-number="<%= week.number %>"></div>
<% end %>

